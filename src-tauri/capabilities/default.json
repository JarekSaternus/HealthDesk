{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for HealthDesk main window",
  "windows": ["main", "break-big", "break-small", "exercise-eye", "exercise-stretch", "water-reminder"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "core:webview:allow-webview-close",
    "core:window:allow-close",
    "shell:allow-open",
    "notification:default",
    "autostart:default",
    "autostart:allow-enable",
    "autostart:allow-disable"
  ]
}
